    <%- include("header") -%>       

    
    <div class="box" id="heading">
        <h1><%= kindOfday %>
        </h1>
    </div>

    <div class="box">

        
        <% for (var i=0;i<newItem.length;i++){ %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%= newItem[i] %>
                </p>
            </div>
        <% } %>

        <form class="item" action="/" method="post">
            <input type="text" name="addItem" placeholder="New Item" autocomplete="off">
            <button type="submit" name="page" value= <%= kindOfday %> >+</button>
        </form>

    </div>

    <%- include("footer") -%>


